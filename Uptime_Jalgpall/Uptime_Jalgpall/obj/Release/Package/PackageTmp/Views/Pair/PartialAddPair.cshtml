@model Uptime_Jalgpall.Models.ViewModelHelper

@{
    var PairNr = Model.HelperIntList[0];
    var Key1 = Model.HelperStringList[0];
    var Key2 = Model.HelperStringList[1];
}

<div class="form-group">
    <div class="col-md-offset-2 col-md-10">
        <h4>Pair @(PairNr).</h4>
        Team 1: @Html.DropDownList(Key1)
        versus
        Team 2: @Html.DropDownList(Key2)
    </div>
</div>